{
  "name": "Mobills",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "build": {
    "icon": "./src/assets/icons/mobills.svg"
  },
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon ./src/assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=x64 --icon ./src/assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Shopping List\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon ./src/assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "electron": "^13.1.7",
    "custom-electron-titlebar": "^3.2.7"
  },
  "devDependencies": {
    "electron-packager": "^15.3.0"
  }
}
